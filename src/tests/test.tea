fn quickread()->int{
	let ch:int = getch();
	let x:int = 0;
	let f:int = 0;
	while ((ch < 48) || (ch > 57)){
		if((ch == 45)){
			f = 1;
		} 
		ch = getch();
	}
	while ((ch >= 48) && (ch <=57)){
		x = x * 10 + ch - 48;
		ch = getch();
	}
	if ((f != 0)){
		ret -x;
	}else{
		ret x;
	}
}

